    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Run Simpsons",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor", "simpsons",
                "--dart-define=FLAVOR=simpsons"
            ]
        },
        {
            "name": "Run The Wire",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor", "thewire",
                "--dart-define=FLAVOR=thewire"
            ]
        }
    ]
}